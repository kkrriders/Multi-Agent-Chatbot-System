{
  "name": "multi-agent-chatbot-system",
  "version": "3.0.0",
  "description": "Production-ready multi-agent AI system with intelligent GPU memory management, real-time monitoring, and breakthrough agent-to-agent communication protocols",
  "main": "start-stable.js",
  "scripts": {
    "start": "node start-stable.js",
    "stop": "node stop-services.js",
    "restart": "npm run stop && sleep 3 && npm start",
    "dev": "concurrently --names \"MANAGER,AGENT-1,AGENT-2,AGENT-3,AGENT-4\" -c \"bgBlue.bold,bgGreen.bold,bgYellow.bold,bgMagenta.bold,bgCyan.bold\" \"npm run start-manager\" \"npm run start-agent-1\" \"npm run start-agent-2\" \"npm run start-agent-3\" \"npm run start-agent-4\"",
    "start-manager": "node src/agents/manager/index.js",
    "start-agent-1": "node src/agents/agent-llama3/index.js",
    "start-agent-2": "node src/agents/agent-mistral/index.js",
    "start-agent-3": "node src/agents/agent-phi3/index.js",
    "start-agent-4": "node src/agents/agent-qwen/index.js",
    "monitor": "node src/monitoring/enhanced-performance-monitor.js",
    "monitor-gpu": "node src/monitoring/gpu-monitor.js",
    "warm-models": "node src/utils/warm-models.js",
    "download-models": "node src/utils/download-models.js",
    "setup-env": "node src/utils/setup-env.js",
    "test": "cd tests && node test-system.js",
    "test-memory": "cd tests && node test-memory.js",
    "test-startup": "cd tests && node test-startup.js",
    "setup": "npm install && npm run setup-env && npm run download-models",
    "clean": "rm -rf logs/*.log && rm -rf memory/users/*.json && rm -rf exports/*.pdf",
    "backup": "tar -czf backup-$(date +%Y%m%d-%H%M%S).tar.gz --exclude=node_modules --exclude=logs --exclude=memory .",
    "health": "curl http://localhost:3099/health && curl http://localhost:3000/status"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/multi-agent-chatbot-system.git"
  },
  "keywords": [
    "multi-agent",
    "chatbot",
    "ai",
    "llm",
    "ollama",
    "gpu-optimization",
    "agent-communication",
    "model-management",
    "websocket",
    "real-time",
    "performance-monitoring",
    "nodejs"
  ],
  "author": "Your Name <your.email@domain.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/multi-agent-chatbot-system/issues"
  },
  "homepage": "https://github.com/your-username/multi-agent-chatbot-system#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "html-pdf": "^3.0.1",
    "morgan": "^1.10.0",
    "ollama": "^0.5.16",
    "socket.io": "^4.8.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
